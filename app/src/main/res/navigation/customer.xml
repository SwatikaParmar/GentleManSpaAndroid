<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/customer"
    app:startDestination="@id/homeCustomerFragment">

    <fragment
        android:id="@+id/homeCustomerFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.home.HomeCustomerFragment"
        android:label="fragment_home_customer"
        tools:layout="@layout/fragment_home_customer" >
        <action
            android:id="@+id/action_homeCustomerFragment_to_serviceFragment"
            app:destination="@id/serviceFragment" />
        <action
            android:id="@+id/action_homeCustomerFragment_to_productFragment"
            app:destination="@id/productFragment" />

    </fragment>
    <fragment
        android:id="@+id/serviceFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.service.ServiceFragment"
        android:label="ServiceFragment"
        tools:layout="@layout/fragment_service">
        <action
            android:id="@+id/action_serviceFragment_to_selectProfessionalFragment"
            app:destination="@id/selectProfessionalFragment" />
        <action
            android:id="@+id/action_serviceFragment_to_serviceDetailFragment"
            app:destination="@id/serviceDetailFragment" />

        <argument
            android:name="categoryId"
            app:argType="integer" />

        <argument
            android:name="selectPosition"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/selectProfessionalFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.selectProfessional.SelectProfessionalFragment"
        android:label="SelectProfessionalFragment"
        tools:layout="@layout/fragment_select_professional">
        <action
            android:id="@+id/action_selectProfessionalFragment_to_makeAppointmentFragment"
            app:destination="@id/makeAppointmentFragment" />

        <action
            android:id="@+id/action_selectProfessionalFragment_to_selectProfessionalServiceFragment"
            app:destination="@id/selectProfessionalServiceFragment" />
    </fragment>
    <fragment
        android:id="@+id/makeAppointmentFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.makeAppointment.MakeAppointmentFragment"
        android:label="MakeAppointmentFragment"
        tools:layout="@layout/fragment_make_appointment">

     <!--   <argument android:name="professionalName"
            app:argType="string"/>
        <argument android:name="professionalImage"
            app:argType="string"/>
        <argument android:name="specialist"
            app:argType="string"/>-->
        <argument android:name="spaServiceId"
            app:argType="integer"/>
        <argument
            android:name="professionalItem"
            app:argType="com.app.gentlemanspa.ui.customerDashboard.fragment.selectProfessional.model.ProfessionalItem" />


    </fragment>
    <fragment
        android:id="@+id/profileCustomerFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.profile.ProfileCustomerFragment"
        android:label="ProfileCustomerFragment"
        tools:layout="@layout/fragment_profile_customer">
        <action
            android:id="@+id/action_profileCustomerFragment_to_editProfileCustomerFragment"
            app:destination="@id/editProfileCustomerFragment" />
    </fragment>
    <fragment
        android:id="@+id/editProfileCustomerFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.editProfile.EditProfileCustomerFragment"
        android:label="EditProfileCustomerFragment"
        tools:layout="@layout/fragment_edit_profile_customer"/>
    <fragment
        android:id="@+id/serviceDetailFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.serviceDetail.ServiceDetailFragment"
        android:label="ServiceDetailFragment"
        tools:layout="@layout/fragment_service_detail">
        <argument
            android:name="serviceId"
            app:argType="integer"
            android:defaultValue="0"/>
        <argument
            android:name="spaDetailId"
            app:argType="integer"
            android:defaultValue="0"/>
    </fragment>
    <fragment
        android:id="@+id/historyCustomerFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.history.HistoryCustomerFragment"
        android:label="HistoryCustomerFragment"
        tools:layout="@layout/fragment_history_customer"/>
    <fragment
        android:id="@+id/productDetailFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.productDetail.ProductDetailFragment"
        android:label="ProductDetailFragment"
        tools:layout="@layout/fragment_product_detail">
        <argument
            android:name="productId"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/productFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.product.ProductFragment"
        android:label="ProductFragment"
        tools:layout="@layout/fragment_product">
        <action
            android:id="@+id/action_productFragment_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />

        <argument
            android:name="mainCategoryId"
            app:argType="integer" />

        <argument
            android:name="selectPosition"
            app:argType="integer" />


    </fragment>

    <fragment
        android:id="@+id/selectProfessionalServiceFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.selectProfessionalService.SelectProfessionalServiceFragment"
        android:label="SelectProfessionalServiceFragment"
        tools:layout="@layout/fragment_select_professional_service">
        <action
            android:id="@+id/action_selectProfessionalServiceFragment_to_anyProfessionalFragment"
            app:destination="@id/anyProfessionalFragment" />

    </fragment>

    <fragment
        android:id="@+id/anyProfessionalFragment"
        android:name="com.app.gentlemanspa.ui.customerDashboard.fragment.anyProfessional.AnyProfessionalFragment"
        android:label="AnyProfessionalFragment"
        tools:layout="@layout/fragment_any_professional">

        <action
            android:id="@+id/action_anyProfessionalFragment_to_makeAppointmentFragment"
            app:destination="@id/makeAppointmentFragment" />
        <argument
            android:name="spaDetailId"
            app:argType="integer"/>
        <argument
            android:name="spaServiceId"
            app:argType="integer"/>
    </fragment>
</navigation>